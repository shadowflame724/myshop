          rel='stylesheet' type='text/css'>
    <!--//fonts-->
    <script type="text/javascript" src="{{ asset('bundles/default/js/move-top.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/default/js/easing.js') }}"></script>
    <script type="text/javascript">
        jQuery(document).ready(function ($) {
            $(".scroll").click(function (event) {
                event.preventDefault();
                $('html,body').animate({scrollTop: $(this.hash).offset().top}, 1000);
            });
        });
    </script>
    <!-- start menu -->
    <script src="{{ asset('bundles/default/js/simpleCart.min.js') }}"></script>
    <link href="{{ asset('bundles/default/css/memenu.css') }}" rel="stylesheet" type="text/css" media="all"/>
    <script type="text/javascript" src="{{ asset('bundles/default/js/memenu.js') }}"></script>
    <script>$(document).ready(function () {
            $(".memenu").memenu();
        });</script>
</head>
<body>
<!--top-header-->
<div class="top-header">
    <div class="container">
        <div class="top-header-main">
            <div class="col-md-4 top-header-left">
                <div class="search-bar">
                    <input type="text" value="Search" onfocus="this.value = '';"
                           onblur="if (this.value == '') {this.value = 'Search';}">
                    <input type="submit" value="">
                </div>
            </div>
            <div class="col-md-4 top-header-middle">
                <a href="index.html"><img src="{{ asset('bundles/default/images/logo-4.png') }}" alt=""/></a>
            </div>
            <div class="col-md-4 top-header-right">
                <div class="cart box_1">
                    <a href="checkout.html">
                        <div class="total">
                            <span class="simpleCart_total"></span> (<span id="simpleCart_quantity"
                                                                          class="simpleCart_quantity"></span> items)
                        </div>
                        <img src="images/cart-1.png" alt=""/>
                    </a>
                    <p><a href="javascript:;" class="simpleCart_empty">Empty Cart</a></p>
                    <div class="clearfix"></div>
                </div>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
</div>
<!--top-header-->
